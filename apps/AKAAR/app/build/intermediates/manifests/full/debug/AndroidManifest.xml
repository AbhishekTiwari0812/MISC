<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhishektiwari.akaar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.abhishektiwari.akaar.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.abhishektiwari.akaar.HomePage"
            android:label="Home" />
        <activity
            android:name="com.example.abhishektiwari.akaar.TodayStat"
            android:label="Performance Today" />
        <activity
            android:name="com.example.abhishektiwari.akaar.CompleteStat"
            android:label="OverAll statistics" />
        <activity
            android:name="com.example.abhishektiwari.akaar.StudentList"
            android:label="Student List" />
    </application>

</manifest>